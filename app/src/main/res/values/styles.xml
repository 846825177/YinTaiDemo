<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="login_share_img">
        <item name="android:layout_width">@dimen/w110dip</item>
        <item name="android:layout_height">@dimen/h110dip</item>
    </style>
    <style name="login_other_txt">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/color_666666</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/h14dip</item>
    </style>
    <style name="style_18_ffffff">
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#ffffff</item>
    </style>
    <style name="style_17_e5004f">
        <item name="android:textSize">17sp</item>
        <item name="android:textColor">#e5004f</item>
    </style>
    <style name="style_14_a7a7a7">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#a7a7a7</item>
    </style>
    <style name="style_13_231f20">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#231f20</item>
    </style>
</resources>
